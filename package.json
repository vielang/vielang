    "clean": "rimraf --glob **/node_modules **/dist **/.next pnpm-lock.yaml **/.tsbuildinfo **/.contentlayer **/.react-email",
    "build": "velite && next build",
    "build:check": "npm run check && npm run build",
    "build:strict": "npm run lint && npm run typecheck && npm run format:check && npm run build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "velite && tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:codegen": "playwright codegen http://localhost:3000",
    "test:report": "playwright show-report",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add",
    "email:dev": "email dev --dir src/components/emails -p 3001",
    "unlighthouse": "pnpm dlx unlighthouse --site https://vielang.vielang.com"
